<% if user_signed_in? %>
  <li class="normal-li"><%= link_to 'My account', edit_user_registration_path %></li>
  <li class="normal-li"><%= link_to 'Log out', destroy_user_session_path, :method=>'delete' %></li>
<% else %>
  <li class="normal-li"><%= link_to 'Log in', new_user_session_path %></li>
  <li class="normal-li"><%= link_to 'Sign up', new_user_registration_path %></li>
<% end %>
